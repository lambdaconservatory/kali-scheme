# This is a kinda gross temporary hack

all: ps-compiler.image

ps-comp-src		:=	$(wildcard $(srcdir)/*.scm) \
				$(wildcard $(srcdir)/*/*.scm) \
				$(wildcard $(srcdir)/*/*/*.scm)

ps-compiler.image: $(ps-comp-src)
	(   echo ',batch' \
	 && echo ',config ,load ../scheme/prescheme/interface.scm' \
	 && echo ',config ,load ../scheme/prescheme/package-defs.scm' \
	 && echo ',exec ,load load-ps-compiler.scm' \
	 && echo ',in prescheme-compiler prescheme-compiler' \
	 && echo ',user (define prescheme-compiler ##)' \
	 && echo ',dump $(objroot)/scheme48/ps-compiler/ps-compiler.image "(Pre-Scheme)"' ) \
	| (cd $(srcdir) && $(objroot)/scheme48/scheme48/scheme48 )

